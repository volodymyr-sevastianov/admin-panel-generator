{
  "columns": {
    "id": {
      "type": "string",
      "maxLength": 255,
      "constraint": "primary key"
    },
    "name": {
      "type": "string",
      "maxLength": 255,
      "constraint": null
    },
    "description": {
      "type": "string",
      "maxLength": 255,
      "constraint": null
    },
    "creationDate": {
      "type": "date",
      "maxLength": null,
      "constraint": null
    },
    "ownerId": {
      "type": "string",
      "maxLength": 255,
      "constraint": "foreign key"
    }
  },
  "relationMappings": {
    "ownerId": {
      "relation": "oneToMany",
      "join": {
        "from": "ownerId",
        "to": "users.id"
      }
    },
    "users": {
      "relation": "manyToMany",
      "join": {
        "from": "projects.id",
        "through": {
          "table": "userProject",
          "from": "userProject.projectId",
          "to": "userProject.userId"
        },
        "to": "users.id",
        "toTable": "users"
      }
    }
  }
